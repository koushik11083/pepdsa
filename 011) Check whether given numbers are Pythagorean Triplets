import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int b=sc.nextInt();
		int c=sc.nextInt();
		a*=a;
		b*=b;
		c*=c;
		if(a+b==c||b+c==a||c+a==b)
		System.out.println("YES");
		else
		System.out.println("NO");
	}
}
