import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		for(int i=1;i<=n;i++)
		{
		    for(int j=n-i;j>=0;j--)
		    {
		        System.out.print("* ");
		    }
		    System.out.println();
		}
		
	}
}
