import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    Scanner sc=new Scanner(System.in);
		int x=sc.nextInt();
		int n=x;
		int c=0;
		while(n>0)
		{
		    int d=n%10;
		    n/=10;
		    c=c*10+d;
		}
		System.out.println(c);
	}
}
