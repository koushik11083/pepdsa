import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int i=1;
		int r=0;
		while(n>0)
		{
		    int k=n%10;
		    r+=(Math.pow(10,k-1)*i);
		    n/=10;
		    i++;
		}
		System.out.println(r);
	}
}
